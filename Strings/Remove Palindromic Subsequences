Method 1->
int removePalindromeSub(string s) {
        int i=0,j=s.size()-1;
        bool isPalindrome=true;
        while(i<=j){
            if(s[i]!=s[j]) isPalindrome=false;
            i++;
            j--;
        } 
        if(isPalindrome) return 1;
        else return 2;
    }
    TC->O(n)
    SC->O(1)

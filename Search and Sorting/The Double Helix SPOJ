Method 1
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++) cin>>a[i];
    int m;
    cin>>m;
    int b[m];
    for(int i=0;i<m;i++) cin>>b[m];
    
    int s1=0,s2=0;
    int i=0,j=0;
    int ma=0;
    while(i<n && j<m){
        if(a[i]<b[j]){
            s1+=a[i];
            i++;
        }
        else if(b[j]<a[i]){
            s2+=b[j];
            j++;
        }
        else{
            ma+=(max(s1,s2)+a[i]);
            s1=0;
            s2=0;
            i++;
            j++;
        }
    }
    while(i<n){
        s1+=a[i];
        i++;
    }
    while(j<m){
        s2+=b[j];
        j++;
    }
    ma+=max(s1,s2);
    cout<<ma<<endl;
    
    return 0;
}
TC->O(n=m)
SC->O(1)
